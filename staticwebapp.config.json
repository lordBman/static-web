{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/admin",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/login",
            "methods": ["GET", "POST"],
            "rewrite": "/.auth/login/aad"
        },
        {
            "route": "/logout",
            "rewrite": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/notfound"
        },
        "401": {
            "rewrite": "/unauthorized"
        }
    }
}